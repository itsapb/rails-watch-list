<div class="container mt-5">

<h1>Create Your Watch List</h1>
<h2>Your List</h2>

<ul>
  <% @lists.each do |list| %>
<li> <%= link_to list.name, list_path(list)%>
<% if list.photo.key? %>
    <%= cl_image_tag(list.photo.key, width: 300, height: 400, crop: :fill) %>
    <% end %></li>
  <% end %>
  </ul>

<div class="btn btn-success">
<%= link_to "Create new list", new_list_path %>
</div>
</div>
